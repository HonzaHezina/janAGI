# ---- Postgres (janagi business DB) ----
# docker-compose uses this as the default database.
# Init scripts create rag.* and analytics.* schemas here.
POSTGRES_DB=janagi
POSTGRES_USER=janagi
POSTGRES_PASSWORD=change_me

# ---- n8n ----
N8N_PORT=5678
N8N_HOST=0.0.0.0
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-domain.com/
N8N_EDITOR_BASE_URL=https://your-domain.com/
N8N_ENCRYPTION_KEY=change_me_to_32_chars_min

# Timezone
TZ=Europe/Prague
GENERIC_TIMEZONE=Europe/Prague

# ---- AI Providers ----
OPENAI_API_KEY=change_me
MISTRAL_API_KEY=change_me

# ---- MindsDB ----
MINDSDB_STORAGE_DIR=/root/mdb_storage
MINDSDB_PG_USER=mindsdb_ro
MINDSDB_PG_PASSWORD=change_me_mindsdb

# ---- OpenClaw (Turbo) ----
OPENCLAW_BASE_URL=http://openclaw:18789
OPENCLAW_GATEWAY_TOKEN=change_me

# ---- n8n API (optional; Workflow Builder) ----
N8N_BASE_URL=http://n8n:5678
N8N_API_KEY=change_me

# ---- n8n UI Operator (optional) ----
N8N_INTERNAL_URL=http://n8n:5678
N8N_OPERATOR_EMAIL=operator@example.com
N8N_OPERATOR_PASSWORD=change_me
